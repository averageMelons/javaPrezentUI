<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.layout.AnchorPane?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.uav.prezentui.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Universitatea de Stiinte Exacte 'Aurel Vlaicu' Arad" style="-fx-font-size: 24.0"/>
    <Label text="Interfata grafica pentru trecerea si vizualizarea prezenteleor" wrapText="true"/>
    <Button fx:id="btnIntPrez" text="Introducere Prezenta" onAction="#onHelloButtonClick" />
    <Button fx:id="btnVizPrez" text="Vizualizare Prezente" />
    <Button fx:id="btnVizAdminPrez" text="Vizualizare Prezente (web, Admin)" onAction="#onVizPrezAdminClick" />
</VBox>
